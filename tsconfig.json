{
   "compilerOptions":{
      "outDir":"./dist",
      "allowJs":true,
      "resolveJsonModule":true,
      "esModuleInterop":false,
      "target":"es6",
      "sourceMap":true,
      "module":"commonjs",
      "moduleResolution":"node",
      "noImplicitAny":false,
      "noImplicitThis":false,
      "types": [
        "node"
      ]
   },
   "include":[
      "./*"
   ],
   "exclude":[
      "node_modules"
   ]
}
